for(int i=0;i<10;i++){
    for(int j=0;j<10;j++){
        p[i][j]=j;
    }
  }
for(int i=0;i<10;i++){
    for(int j=0;j<10;j++){
        cout<<p[i][j]<<"\t";
    }
    cout<<"\n";
  }
