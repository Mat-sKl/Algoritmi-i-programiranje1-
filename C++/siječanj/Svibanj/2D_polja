for(int i=0;i<4;i++){
    for(int j=0;j<3;j++){
        cin>>p[i][j];
    }

}
for(int i=0;i<4;i++){
    for(int j=0;j<3;j++){
       cout<<p[i][j]<<" ";
   }
cout<<endl;
}
}
