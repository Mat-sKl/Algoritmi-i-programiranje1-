#include <iostream>

using namespace std;

int main()
{
int p[4][3];
/*p[0][0]=1
p[0][1]=2
p[0][2]=3
p[1][0]=4
p[1][1]=5
p[1][2]=6
p[2][0]=7
p[2][1]=8
p[2][2]=9
p[3][0]=10
p[3][1]=11
p[3][2]=12*/
for(int i=0;i<4;i++){
    for(int j=0;j<3;j++){
        cin>>p[i][j];
    }

}
for(int i=0;i<4;i++){
    for(int j=0;j<3;j++){
       cout<<p[i][j]<<" ";
   }
   cout<<endl;
}
for(int i=0;i<1;i++){                 //ili cout<<p[0][i] for(int i=0;i<3;i++)
    for(int j=0;j<3;j++){
       cout<<p[i][j]<<" ";
   }
}
//------------------------------------------------------------
cout<<endl;
for(int i=0;i<3;i++){
       cout<<p[1][i]<<" ";

}
//------------------------------------------------------------
cout<<endl;
for(int i=0;i<3;i++){
       cout<<p[2][i]<<" ";

}
//------------------------------------------------------------
cout<<endl;
for(int i=2;i>-1;i--){
       cout<<p[2][i]<<" ";

}
//------------------------------------------------------------
cout<<endl;
for(int i=0;i<4;i++){
       cout<<p[i][0]<<" ";

}
//------------------------------------------------------------
cout<<endl;
for(int i=0;i<3;i++){
    cout<<p[i][i]<<" ";
}
//------------------------------------------------------------
cout<<endl;
for(int i=2;i>=0;i--){
    cout<<p[i][i]<<" "; //ili cout<<p[i][3-i-1];
}
//------------------------------------------------------------
cout<<endl;
for(int i=2;i>=0;i--){
    cout<<p[i][3-i-1]<<" ";
}
//------------------------------------------------------------
cout<<endl;
for(int i=0;i<4;i++){
    for(int j=0;j<3;j++){
       cout<<p[i][j]<<"\t ";
   }
   cout<<endl;
}
//------------------------------------------------------------
cout<<endl;
for(int i=0;i<4;i++){
    for(int j=0;j<3;j++){
            if(i%2==0){
       cout<<p[i][j+1]<<" ";
       }
       else if(i%2!=0){
       cout<<p[i][j-1]<<" ";
       }
   }
   cout<<endl;
}
}

