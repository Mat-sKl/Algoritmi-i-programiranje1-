#include <iostream>

using namespace std;

int main()
{
int p[4][3];
/*p[0][0]=1
p[0][1]=2
p[0][2]=3
p[1][0]=4
p[1][1]=5
p[1][2]=6
p[2][0]=7
p[2][1]=8
p[2][2]=9
