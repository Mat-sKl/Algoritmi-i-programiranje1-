#include <iostream>
using namespace std;

int main()
{
int p[14][100];
for(int i=0;i<14;i++){
    for(int j=0;j<100;j++){
        cin>>p[i][j];
        if(p[i][j]==0)
           break;
   }
}

for(int i=0;i<14;i++){
    for(int j=0;j<100;j++){
        if(p[i][j]==0)
           break;
       cout<<p[i][j]<<" ";
   }
cout<<endl;
}
}
